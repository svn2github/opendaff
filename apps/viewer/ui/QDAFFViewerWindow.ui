              <widget class="QDoubleSpinBox" name="doubleSpinBox_Theta">
               <property name="toolTip">
                <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;A &lt;span style=&quot; font-weight:600; color:#55aa00;&quot;&gt;green&lt;/span&gt; background indicates a phi &lt;span style=&quot; font-style:italic;&quot;&gt;and&lt;/span&gt; theta angle pair that represents a &lt;span style=&quot; font-weight:600;&quot;&gt;valid direction&lt;/span&gt; that is covered by the currently opened DAFF file.&lt;/p&gt;&lt;p&gt;A &lt;span style=&quot; font-weight:600; color:#ff0000;&quot;&gt;red&lt;/span&gt; background indicates that thes direction is&lt;span style=&quot; font-weight:600;&quot;&gt; out of bounds&lt;/span&gt;.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
